{
  "name": "@morphdb/morph-client",
  "description": "Client for Morph's Data API",
  "version": "0.0.8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Atsuki",
  "module": "dist/morph-client.esm.js",
  "size-limit": [
    {
      "path": "dist/morph-client.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/morph-client.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "url-join": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/useMorph/morph-client-js.git"
  },
  "bugs": {
    "url": "https://github.com/useMorph/morph-client-js/issues"
  },
  "homepage": "https://github.com/useMorph/morph-client-js#readme"
}
